




	INCLUDE "z80_crt0.hdr"


	SECTION	code_compiler

._func
	ld	hl,(_funcptr)
	ld	de,(_funcptr+2)
	ld	d,0
	push	de
	push	hl
	ld	hl,1	;const
	ld	de,0
	push	de
	push	hl
	ld	hl,2	;const
	push	hl
	ld	hl,6
	add	hl,de
	ld	e,(hl)
	inc	hl
	ld	d,(hl)
	inc	hl
	ld	l,(hl)
	ex	de,hl
	ld	a,3
	call	l_farcall
	pop	bc
	pop	bc
	pop	bc
	pop	bc
	pop	bc
	call	l_gchar
	ret



._func2
