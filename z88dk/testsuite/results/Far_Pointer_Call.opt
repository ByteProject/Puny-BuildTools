	ld	hl,1	;const
	ld	de,0
	push	de
	push	hl
	ld	hl,2	;const
	push	hl
	ld	hl,6
	add	hl,de
	ld	e,(hl)
	inc	hl
	ld	d,(hl)
	inc	hl
	ld	l,(hl)
	ex	de,hl
	ld	a,3
	call	l_farcall
	pop	bc
	pop	bc
	pop	bc
	pop	bc
	pop	bc
	call	lp_gchar
	call	l_sxt
