._div
	ld	hl,2	;const
	add	hl,sp
	call	l_glong
	ld	bc,0
	push	bc
	ld	bc,2
	push	bc
	call	l_long_div
	ret



._div2
	ld	hl,2	;const
	add	hl,sp
	call	l_glong
	bit	7,d
	jr	z,i_2
	call	l_inclong
.i_2
	sra	d
	rr	e
	rr	h
	rr	l
	ret



._div3
	ld	hl,(_g)
	ld	de,(_g+2)
	push	de
	push	hl
	ld	hl,6	;const
	add	hl,sp
	call	l_glong
	call	l_long_div
	ret



