




	INCLUDE "z80_crt0.hdr"


	SECTION	code_compiler

._mul
	ld	hl,2	;const
	add	hl,sp
	call	l_glong
	add	hl,hl
	rl	e
	rl	d
	ret



._muli
	pop	bc
	pop	hl
	push	hl
	push	bc
	add	hl,hl
	ret



._muli2
	ld	hl,(_k)
	push	hl
	ld	hl,4	;const
	add	hl,sp
	call	l_glong
	exx
	pop	hl
	call	l_int2long_s
	push	de
	push	hl
	exx
	call	l_long_mult
	ret



